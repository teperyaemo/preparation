<Window x:Class="preparation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:preparation"
        mc:Ignorable="d"
        Title="Попрыженок" Height="450" Width="800"
        MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="592"/>
        </Grid.ColumnDefinitions>
        <StackPanel Background="#43DCFE"/>
        <Frame Name="MainFrame" Grid.Column="1"/>
        <Label Name="Head" Grid.Column="0"  Content="Агенты" HorizontalAlignment="Center" VerticalAlignment="Top" FontFamily="Comic Sans MS"/>
        <Image Source="/Resources/Попрыженок.png" VerticalAlignment="Top" Margin="0,37,0,0"/>

    </Grid>
</Window>
